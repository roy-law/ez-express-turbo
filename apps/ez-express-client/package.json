{
  "name": "ez-express-client",
  "private": true,
  "version": "0.3.0",
  "scripts": {
    "client:dev": "vite",
    "client:build": "tsc && vite build",
    "server:dev": "ts-node-dev --project tsconfig.server.json server",
    "server:build": "tsc --project tsconfig.server.json",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "start": "node dist/server.js",
    "build": "npm run server:build && npm run client:build",
    "dev": "portkill 3001 && portkill 5173 && concurrently \"npm run server:dev\" \"npm run client:dev\""
  },
  "homepage": ".",
  "dependencies": {
    "@auth0/auth0-react": "^2.0.0",
